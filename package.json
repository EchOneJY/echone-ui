{
  "scripts": {
    "dev": "vite",
    "dev:docs": "pnpm -F @echone-ui/docs run dev",
    "build": "vue-tsc -b && vite build",
    "build:docs": "pnpm -F @echone-ui/docs run build",
    "preview": "vite preview",
    "test:unit": "vitest run --dom",
    "postinstall": "pnpm -r run stub --if-present",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "lint": "eslint . --fix --cache",
    "format": "prettier . --write --cache --log-level warn"
  },
  "devDependencies": {
    "@echone-ui/commitlint-config": "workspace:*",
    "@echone-ui/components": "workspace:*",
    "@echone-ui/eslint-config": "workspace:*",
    "@echone-ui/prettier-config": "workspace:*",
    "@echone-ui/stylelint-config": "workspace:*",
    "@echone-ui/tailwind-config": "workspace:*",
    "@echone-ui/tsconfig": "workspace:*",
    "@types/node": "^22.7.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/test-utils": "^2.4.6",
    "cspell": "^8.15.4",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.3",
    "vue": "^3.5.10",
    "vue-tsc": "^2.1.6"
  },
  "packageManager": "pnpm@9.6.0"
}
